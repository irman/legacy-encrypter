{
    "name": "irman/legacy-encrypter",
    "description": "The legacy version of the Laravel mcrypt encrypter.",
    "license": "MIT",
    "homepage": "https://github.com/irman/legacy-encrypter",
    "support": {
        "issues": "https://github.com/irman/legacy-encrypter",
        "source": "https://github.com/irman/legacy-encrypter"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Irman Ahmad",
            "email": "irman@outlook.my"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "illuminate/contracts": "5.7.*",
        "illuminate/support": "5.7.*",
        "paragonie/random_compat": "~1.4|~2.0",
        "phpseclib/mcrypt_compat": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Irman\\LegacyEncrypter\\": "src/"
        }
